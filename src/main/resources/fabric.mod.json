{
  "schemaVersion": 1,
  "id": "bypass-anti-direct-connect",
  "version": "${version}",
  "name": "Bypass AntiDirectConnect",
  "description": "An addon template for the Meteor addons that bypasses AntiDirectConnect",
  "authors": [
    "19MisterX98"
  ],
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "misterx.bypassADC.Addon"
    ]
  },
  "mixins": [
    "direct-connect-spoof.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
